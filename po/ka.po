#: activity/activity.info:2
msgid "ColorDeducto"
msgstr "ColorDeducto"

#: activity/activity.info:3
msgid "a game based on logical induction"
msgstr ""

#: ColorDeductoActivity.py:91
#, python-format
msgid "Resuming level %d"
msgstr "ვაგრძელებ დონეს %d"

#: ColorDeductoActivity.py:115 ColorDeductoActivity.py:182
#: ColorDeductoActivity.py:212
msgid "Start a new game."
msgstr "ახალი თამაშის დაწყება."

#: ColorDeductoActivity.py:121 ColorDeductoActivity.py:253
msgid "The pattern matches the rule."
msgstr "ნიმუში ემთხვევა წესს."

#: ColorDeductoActivity.py:125 ColorDeductoActivity.py:255
msgid "The pattern does not match the rule."
msgstr "ნიმუში არ ემთხვევა წესს."

#: ColorDeductoActivity.py:131 ColorDeductoActivity.py:251
msgid "Explore some examples."
msgstr "დაათვალიერეთ ზოგიერთი მაგალითი."

#: ColorDeductoActivity.py:140
msgid "Load a custom level."
msgstr "მორგებული დონის ჩატვირთვა."

#: ColorDeductoActivity.py:160 ColorDeductoActivity.py:171
#: ColorDeductoActivity.py:256 ColorDeductoActivity.py:419
#, python-format
msgid "Playing level %d"
msgstr "თამაშობთ დონეს %d"

#: ColorDeductoActivity.py:162
msgid "Only sharer can start a new game."
msgstr "ახალი თამაშის დაწყება, მხოლოდ, გამზიარებელს შეუძლია."

#: ColorDeductoActivity.py:169
msgid "Game over."
msgstr "თამაში დასრულდა."

#: ColorDeductoActivity.py:184 ColorDeductoActivity.py:214
msgid "Wait for sharer to start a new game."
msgstr "დაელოდეთ გამზიარებელს, რომ ახალ თამაში დაიწყოს."

#: ColorDeductoActivity.py:195 ColorDeductoActivity.py:224
#, python-format
msgid "%d correct answers."
msgstr "%d სწორი პასუხი."

#: ColorDeductoActivity.py:201
msgid "Pattern was false."
msgstr "ნიმუში ცრუ იყო."

#: ColorDeductoActivity.py:230
msgid "Pattern was true."
msgstr "ნიმუში ჭეშმარიტი იყო."

#: ColorDeductoActivity.py:241
msgid "Resume play"
msgstr "თამაშის გაგრძელება"

#: ColorDeductoActivity.py:243
msgid "Show a pattern that matches the rule."
msgstr "ნიმუშის ჩვენება, რომელიც წესს ემთხვევა."

#: ColorDeductoActivity.py:245
msgid "Show a pattern that does not match the rule."
msgstr "აჩვენებს ნიმუშს, რომელიც წესს არ ემთხვევა."

#: ColorDeductoActivity.py:247
#, python-format
msgid "Explore patterns with the %s and %s buttons."
msgstr "დაათვალიერეთ ნიმუშები %s და %s ღილაკებით."

#: ColorDeductoActivity.py:263
msgid "Load a \"True\" pattern generator from the journal"
msgstr "\"ჭეშმარიტი\" ნიმუშის გენერატორის ჩატვირთვა ჟურნალიდან"

#: ColorDeductoActivity.py:270
msgid "Load a \"False\" pattern generator from the journal"
msgstr "\"ცრუ\" ნიმუშის გენერატორის ჩატვირთვა ჟურნალიდან"

#: ColorDeductoActivity.py:276
msgid "New level added"
msgstr "ახალი დონე დამატებულია"

#: ColorDeductoActivity.py:122
msgid "Game"
msgstr "თამაში"

#: ColorDeductoActivity.py:200 ColorDeductoActivity.py:230
msgid "Click on new game button to begin."
msgstr "დასაწყებად, დააწკაპუნეთ ახალი თამაშის ღილაკზე."
